{
  "name": "algoliasearch-client-javascript",
  "version": "0.0.0",
  "scripts": {
    "generate:openapi": "openapi-generator-cli generate --generator-key client",
    "generate:swagger": "swagger-codegen generate -c swagger.json -i spec.yml -l typescript-axios -o output/client-swagger/",
    "generate": "yarn generate:openapi && yarn generate:swagger",
    "install:client-openapi": "cd output/client-openapi && yarn install && yarn build && cd ../..",
    "install:client-swagger": "cd output/client-swagger && yarn install && yarn build && cd ../..",
    "install-clients": "yarn install:client-openapi && yarn install:client-swagger",
    "test:build": "tsc",
    "test": "yarn test:build && node dist/app.js"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "2.4.13",
    "prettier": "2.4.1",
    "typescript": "4.4.4"
  },
  "dependencies": {
    "algoliasearch-client-javascript": "file:output/client-swagger/",
    "algoliasearch-client-javascript-openapi": "file:output/client-openapi/",
    "algoliasearch-client-javascript-swagger": "file:output/client-swagger/"
  }
}
