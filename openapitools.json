{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "generator-cli": {
    "version": "5.2.1",
    "generators": {
      "javascript-client": {
        "generatorName": "typescript-node",
        "templateDir": "#{cwd}/templates/javascript/",
        "config": "#{cwd}/openapitools.json",
        "apiPackage": "client-search",
        "output": "#{cwd}/clients/algoliasearch-client-javascript",
        "glob": "specs/spec.yml",
        "gitHost": "algolia",
        "gitUserId": "algolia",
        "gitRepoId": "algoliasearch-client-javascript",
        "additionalProperties": {
          "modelPropertyNaming": "original",
          "supportsES6": true,
          "npmName": "algoliasearch-client-javascript",
          "npmVersion": "5.0.0"
        }
      }
    }
  }
}
