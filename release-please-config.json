{
  "$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json",
  "plugins": [
    "node-workspace",
    {
      "type": "linked-versions",
      "group-name": "javascript-common",
      "components": [
        "client-common",
        "requester-browser-xhr",
        "requester-node-http",
        "requester-fetch"
      ]
