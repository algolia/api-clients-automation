{
  "$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json",
  "bootstrap-sha": "51658367bc04bc258405fe48c882f813ba150600",
  "plugins": [
    "node-workspace"
  ],
  "extra-files": [
    {
      "type": "json",
      "path": "config/clients.config.json",
      "jsonpath": "$.*.packageVersion",
      "glob": true
    }
  ],
  "packages": {
    "clients/algoliasearch-client-javascript": {
      "release-type": "node",
      "prerelease": true,
      "extra-files": [
        {
          "type": "json",
          "path": "../../config/clients.config.json",
          "jsonpath": "$.javascript.utilsPackageVersion",
          "glob": true
        },
        {
          "type": "json",
          "path": "../../config/openapitools.json",
          "jsonpath": "$.generator-cli.generators.*.additionalProperties.packageVersion",
          "glob": true
        }
      ]
    },
    "clients/algoliasearch-client-java-2": {
      "release-type": "java",
      "package-name": "algoliasearch-client-java-2",
      "prerelease": true,
      "extra-files": [
        {
          "type": "json",
          "path": "/config/clients.config.json",
          "jsonpath": "$.java.packageVersion"
        }
      ]
    },
    "clients/algoliasearch-client-php": {
      "release-type": "php",
      "package-name": "algoliasearch-client-php",
      "prerelease": true,
      "extra-files": [
        {
          "type": "json",
          "path": "../../config/clients.config.json",
          "jsonpath": "$.php.packageVersion",
          "glob": true
        }
      ]
    }
  }
}
