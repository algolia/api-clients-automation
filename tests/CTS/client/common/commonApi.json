[
  {
    "testName": "calls api with correct user agent",
    "steps": [
      {
        "type": "method",
        "object": "$client",
        "path": "post",
        "parameters": {
          "path": "/test"
        },
        "expected": {
          "type": "userAgent",
          "match": "^Algolia for ${{languageCased}} \\(\\d+\\.\\d+\\.\\d+.*\\); ${{clientPascalCase}} \\(\\d+\\.\\d+\\.\\d+.*\\); [a-zA-Z. ]+\\(.*\\)$"
        }
      }
    ]
  },
  {
    "testName": "calls api with correct timeouts",
    "steps": [
      {
        "type": "method",
        "object": "$client",
        "path": "post",
        "parameters": {
          "path": "/test"
        },
        "expected": {
          "type": "timeouts",
          "match": {
            "connectTimeout": 2000,
            "responseTimeout": 30000
          }
        }
      }
    ]
  }
]
