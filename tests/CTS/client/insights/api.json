[
  {
    "testName": "calls api with correct user agent",
    "steps": [
      {
        "type": "method",
        "object": "$client",
        "path": "pushEvents",
        "parameters": {
          "events": []
        },
        "expected": {
          "testSubject": "userAgent",
          "match": {
            "regexp": "^Algolia for (Java|JavaScript) \\(\\d+\\.\\d+\\.\\d+\\)(; (\\w)+ (\\(\\d+\\.\\d+\\.\\d+\\))?)*(; Insights (\\(\\d+\\.\\d+\\.\\d+\\)))(; (\\w)+ (\\(\\d+\\.\\d+\\.\\d+\\))?)*$"
          }
        }
      }
    ]
  },
  {
    "testName": "calls api with correct timeouts",
    "steps": [
      {
        "type": "method",
        "object": "$client",
        "path": "pushEvents",
        "parameters": {
          "events": []
        },
        "expected": {
          "match": {
            "objectContaining": {
              "connectTimeout": 2,
              "responseTimeout": 30
            }
          }
        }
      }
    ]
  }
]
