[
  {
    "testName": "calls api with correct host",
    "steps": [
      {
        "type": "method",
        "object": "$client",
        "path": "getRecommendations",
        "parameters": {
          "requests": []
        },
        "expected": {
          "match": {
            "objectContaining": {
              "host": "test-app-id-dsn.algolia.net"
            }
          }
        }
      }
    ]
  },
  {
    "testName": "calls api with correct user agent",
    "steps": [
      {
        "type": "method",
        "object": "$client",
        "path": "getRecommendations",
        "parameters": {
          "requests": []
        },
        "expected": {
          "testSubject": "userAgent",
          "match": {
            "regexp": "^Algolia for (Java|JavaScript) \\(\\d+\\.\\d+\\.\\d+\\)(; (\\w)+ (\\(\\d+\\.\\d+\\.\\d+\\))?)*(; Recommend (\\(\\d+\\.\\d+\\.\\d+\\)))(; (\\w)+ (\\(\\d+\\.\\d+\\.\\d+\\))?)*$"
          }
        }
      }
    ]
  },
  {
    "testName": "calls api with correct timeouts",
    "steps": [
      {
        "type": "method",
        "object": "$client",
        "path": "getRecommendations",
        "parameters": {
          "requests": []
        },
        "expected": {
          "match": {
            "objectContaining": {
              "connectTimeout": 2,
              "responseTimeout": 5
            }
          }
        }
      }
    ]
  }
]
