[
  {
    "testName": "calls api with correct user agent",
    "steps": [
      {
        "type": "method",
        "object": "$client",
        "path": "getAverageClickPosition",
        "parameters": {
          "index": "my-index"
        },
        "expected": {
          "type": "userAgent",
          "match": "^Algolia for (Java|JavaScript) \\(\\d+\\.\\d+\\.\\d+\\)(; [a-zA-Z. ]+ (\\(\\d+\\.\\d+\\.\\d+\\))?)*(; Analytics (\\(\\d+\\.\\d+\\.\\d+\\)))(; [a-zA-Z. ]+ (\\(\\d+\\.\\d+\\.\\d+\\))?)*$"
        }
      }
    ]
  },
  {
    "testName": "calls api with correct timeouts",
    "steps": [
      {
        "type": "method",
        "object": "$client",
        "path": "getAverageClickPosition",
        "parameters": {
          "index": "my-index"
        },
        "expected": {
          "type": "timeouts",
          "match": {
            "connectTimeout": 2,
            "responseTimeout": 5
          }
        }
      }
    ]
  }
]
