[
  {
    "testName": "calls api with correct user agent",
    "steps": [
      {
        "type": "method",
        "object": "$client",
        "path": "addABTests",
        "parameters": {
          "name": "test",
          "variant": [
            {
              "index": "my-test-index",
              "trafficPercentage": 90
            }
          ],
          "endAt": "2022-02-01T13:37:01Z"
        },
        "expected": {
          "type": "userAgent",
          "match": "^Algolia for (Java|JavaScript) \\(\\d+\\.\\d+\\.\\d+\\)(; [a-zA-Z. ]+ (\\(\\d+\\.\\d+\\.\\d+\\))?)*(; Abtesting (\\(\\d+\\.\\d+\\.\\d+\\)))(; [a-zA-Z. ]+ (\\(\\d+\\.\\d+\\.\\d+\\))?)*$"
        }
      }
    ]
  },
  {
    "testName": "calls api with correct timeouts",
    "steps": [
      {
        "type": "method",
        "object": "$client",
        "path": "addABTests",
        "parameters": {
          "name": "test",
          "variant": [
            {
              "index": "my-test-index",
              "trafficPercentage": 90
            }
          ],
          "endAt": "2022-02-01T13:37:01Z"
        },
        "expected": {
          "type": "timeouts",
          "match": {
            "connectTimeout": 2,
            "responseTimeout": 30
          }
        }
      }
    ]
  }
]
