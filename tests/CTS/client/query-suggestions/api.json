[
  {
    "testName": "calls api with correct user agent",
    "steps": [
      {
        "type": "method",
        "object": "$client",
        "path": "createConfig",
        "parameters": {
          "indexName": "my-index-name"
        },
        "expected": {
          "testSubject": "userAgent",
          "match": {
            "regexp": "^Algolia for (Java|JavaScript) \\(\\d+\\.\\d+\\.\\d+\\)(; (\\w)+ (\\(\\d+\\.\\d+\\.\\d+\\))?)*(; QuerySuggestions (\\(\\d+\\.\\d+\\.\\d+\\)))(; (\\w)+ (\\(\\d+\\.\\d+\\.\\d+\\))?)*$"
          }
        }
      }
    ]
  },
  {
    "testName": "calls api with correct timeouts",
    "steps": [
      {
        "type": "method",
        "object": "$client",
        "path": "createConfig",
        "parameters": {
          "indexName": "my-index-name"
        },
        "expected": {
          "match": {
            "objectContaining": {
              "connectTimeout": 2,
              "responseTimeout": 30
            }
          }
        }
      }
    ]
  }
]
