[
  {
    "testName": "client throws with invalid parameters",
    "autoCreateClient": false,
    "steps": [
      {
        "type": "createClient",
        "parameters": {
          "apiKey": "blah"
        },
        "expected": {
          "error": "appId is missing!"
        }
      }
    ]
  },
  {
    "testName": "sets user agent",
    "steps": [
      {
        "type": "method",
        "object": "$client",
        "path": ["setUserAgent"],
        "parameters": ["hello"]
      },
      {
        "type": "method",
        "object": "$client",
        "path": ["getUserAgent"],
        "expected": { "match": "hello" }
      }
    ]
  }
]
