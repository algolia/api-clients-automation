[
  {
    "testName": "calls api with correct user agent",
    "steps": [
      {
        "type": "method",
        "object": "$client",
        "path": "fetchUserProfile",
        "parameters": {
          "userID": "user1",
          "params": {
            "modelsToRetrieve": [
              "funnel_stage"
            ]
          }
        },
        "expected": {
          "type": "userAgent",
          "match": "^Algolia for (Java|JavaScript) \\(\\d+\\.\\d+\\.\\d+\\)(; [a-zA-Z. ]+ (\\(\\d+\\.\\d+\\.\\d+\\))?)*(; Predict (\\(\\d+\\.\\d+\\.\\d+\\)))(; [a-zA-Z. ]+ (\\(\\d+\\.\\d+\\.\\d+\\))?)*$"
        }
      }
    ]
  },
  {
    "testName": "calls api with correct timeouts",
    "steps": [
      {
        "type": "method",
        "object": "$client",
        "path": "fetchUserProfile",
        "parameters": {
          "userID": "user1",
          "params": {
            "modelsToRetrieve": [
              "funnel_stage"
            ]
          }
        },
        "expected": {
          "type": "timeouts",
          "match": {
            "connectTimeout": 2000,
            "responseTimeout": 30000
          }
        }
      }
    ]
  }
]
