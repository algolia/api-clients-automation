  },
  "devDependencies": {
    "@arethetypeswrong/cli": "0.15.4",
    "@babel/preset-env": "7.25.4",
    "@babel/preset-typescript": "7.24.7",
    "@types/jest": "29.5.12",
    "@types/node": "22.5.1",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "publint": "0.2.10",
    "ts-jest": "29.2.5",
    "tsup": "8.2.4",
    "typescript": "5.5.4",
    "xhr-mock": "2.5.1"
  },
  "engines": {
    "node": ">= 14.0.0"
  }
