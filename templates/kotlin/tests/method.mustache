                {{method}}(
                {{#parametersWithDataType}}
                {{> tests/request_param}}
                {{/parametersWithDataType}}
                {{#hasRequestOptions}}
                requestOptions = RequestOptions(
                    {{#requestOptions.queryParameters}}
                    urlParameters = buildMap {
                        {{#parametersWithDataType}}
                        put("{{{key}}}", {{> tests/param_value}})
                        {{/parametersWithDataType}}
