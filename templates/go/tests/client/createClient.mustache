cfg = {{clientPrefix}}.Configuration{
  Configuration: transport.Configuration{
    AppID:     "{{parametersWithDataTypeMap.appId.value}}",
    ApiKey:    "{{parametersWithDataTypeMap.apiKey.value}}",
    Requester: echo,
  },{{#hasRegionalHost}}{{#parametersWithDataTypeMap.region.value.size}}
  Region: {{clientPrefix}}.{{#lambda.uppercase}}{{parametersWithDataTypeMap.region.value}}{{/lambda.uppercase}},{{/parametersWithDataTypeMap.region.value.size}}{{/hasRegionalHost}}
}
client, {{#isError}}err{{/isError}}{{^isError}}_{{/isError}} ={{clientPrefix}}.NewClientWithConfig(cfg)
