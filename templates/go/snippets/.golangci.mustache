version: "2"
run:
  concurrency: 2
linters:
  disable:
    - ineffassign
    - staticcheck
    - unused
  exclusions:
    generated: disable
    presets:
      - comments
      - common-false-positives
      - legacy
      - std-error-handling
    paths:
      - third_party$
      - builtin$
      - examples$
formatters:
  exclusions:
    generated: disable
    paths:
      - third_party$
      - builtin$
      - examples$
